<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Request_expert">

    <TextView
        android:id="@+id/topbar_request_exeprt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#03A9F4"
        android:text="@string/medi_app_request_an_expert"
        android:textAlignment="center"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/call_insurance_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="insurance"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/insurance_call"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="138dp"
        android:layout_height="200dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="68dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toStartOf="@+id/call_insurance_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topbar_request_exeprt"
        app:srcCompat="@drawable/ic_insurance_prov" />

    <ImageView
        android:id="@+id/insurance_call"
        android:layout_width="87dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/topbar_request_exeprt"
        app:layout_constraintTop_toBottomOf="@+id/call_insurance_name"
        app:srcCompat="@drawable/ic_phone" />

    <ImageView
        android:id="@+id/insurance_email"
        android:layout_width="87dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/insurance_call"
        app:layout_constraintStart_toStartOf="@+id/insurance_call"
        app:layout_constraintTop_toBottomOf="@+id/insurance_call"
        app:srcCompat="@drawable/ic_email" />

    <ImageView
        android:id="@+id/email_gp_img"
        android:layout_width="87dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/gp_call"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/gp_call"
        app:layout_constraintTop_toBottomOf="@+id/gp_call"
        app:srcCompat="@drawable/ic_email" />

    <ImageView
        android:id="@+id/gp_call"
        android:layout_width="87dp"
        android:layout_height="57dp"

        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/insurance_email"
        app:layout_constraintTop_toBottomOf="@+id/call_gpname"
        app:srcCompat="@drawable/ic_phone" />

    <TextView
        android:id="@+id/call_insurance_number"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="1234567"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/insurance_call"
        app:layout_constraintEnd_toStartOf="@+id/insurance_call"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/insurance_call" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="138dp"
        android:layout_height="200dp"
        android:layout_marginTop="68dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/ic_call_gp" />

    <TextView
        android:id="@+id/call_gpname"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="GP's Name"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/gp_call"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/call_gps_number"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="1234567"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/gp_call"
        app:layout_constraintEnd_toStartOf="@+id/gp_call"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/gp_call" />

    <Button
        android:id="@+id/request_expertbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="103dp"
        android:backgroundTint="#03A9F4"
        android:text="cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <TextView
        android:id="@+id/expert_in_emailTV"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="email"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/insurance_email"
        app:layout_constraintEnd_toStartOf="@+id/insurance_email"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/insurance_email" />

    <TextView
        android:id="@+id/expert_gp_emailTV"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="email"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/email_gp_img"
        app:layout_constraintEnd_toStartOf="@+id/email_gp_img"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/email_gp_img" />

    <View
        android:id="@+id/divider4"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/call_gpname"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <View
        android:id="@+id/divider5"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/request_expertbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

</androidx.constraintlayout.widget.ConstraintLayout>